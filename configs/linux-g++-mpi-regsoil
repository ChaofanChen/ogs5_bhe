MPI              = yes


CXX_LIBS         = -lstdc++


CXX_LDFLAGS      = -O3
CXX_LD           = mpicxx

CXX_OPT_FLAGS    = -O3
CXX_DEBUG_FLAGS  = 
CXX_WARN_FLAGS   =
CXX_FLAGS        = -DUSE_MPI_REGSOIL -DBENCHMARKING -DUSE_TOKENBUF -DGCC $(CXX_OPT_FLAGS) $(CXX_DEBUG_FLAGS) -DRANDOM_WALK
CXX              = mpicxx

