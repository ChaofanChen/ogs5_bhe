#!/bin/sh
sed 's/SMTM0099/MMTM0699/g' <mod_0099.c >xxx
sed 's/0099/0699/g' <xxx >mod_0699.c
sed 's/SMTM0099/MMTM0699/g' <mod_0099.h >xxx
sed 's/0099/0699/g' <xxx >mod_0699.h
sed 's/SMTM0099/MMTM0699/g' <loop0099.c >xxx
sed 's/0099/0699/g' <xxx >loop0699.c
sed 's/SMTM0099/MMTM0699/g' <loop0099.h >xxx
sed 's/0099/0699/g' <xxx >loop0699.h
more makemod
rm -f xxx
